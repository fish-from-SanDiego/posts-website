<footer class="{{footerClasses.footer}}">
  <div class="{{footerClasses.footerInner}}">
    <div class="{{footerClasses.footerInnerContent}}">
      <div class="{{footerClasses.linksContainer}}">
        {{#each footerLinks}}
          <a class="{{../footerClasses.menuLink}}" href="{{this.url}}">{{this.text}}</a>
        {{/each}}
        <a class="{{footerClasses.menuLink}}">
          Запрос занял {{requestHandleTimeMs}} мс на обработку
        </a>
      </div>
    </div>
  </div>
</footer>